doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width,initial-scale=1")
		title Catico
		link(rel="stylesheet", type="text/css", href="style.css")
	body
		- var imgpath = "img/"
		- var projectPath = "img/projects/"
		.wrapper
			main.main
				//- .overlay.sec1
