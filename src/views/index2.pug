				section.section.intro
					h1 Catico
				section.section.proj
					ul.proj_list
						- for(var i=1; i<13; i++)
							li
								img.proj_img(src=projectPath+"pic"+i+".jpeg")
								.proj_overlay
									.proj_title Project XYZ
									.proj_descr This is very nice project. I have been working on this project for a very long time. It is amazing. Having many great features and various effects.
									.proj_links
									.proj_tags #html #css
				section.section
					.section_left
					.section_top
						h2.section_bgtitle.section_bgtitle__process Creative process
						.process
							.process_el
								.process_icon.bulb
								h3.process_title Think
								.process_line
									svg(width="200", height="200")
										line(x1="0", y1="0", x2="600", y2="0")
							.process_el
								.process_icon.pen
								h3.process_title Design
							.process_el
								.process_icon.brush
								h3.process_title Engineer
							.process_el
								.process_icon.globe
								h3.process_title Develop
